<Window x:Class="AutoApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoApp"
        mc:Ignorable="d"
        Title="Авторизация" Height="226.333" Width="442" Background="#FFB5D5CA"
        Loaded="Window_Loaded">
    <Grid>
        <TextBox x:Name="username" Height="23" Margin="269,36,28,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBox x:Name="password" Height="23" Margin="269,77,28,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Button x:Name="Login" Content="Вход" Margin="269,150,28,0" VerticalAlignment="Top" Click="Login_Click"/>
        <Label HorizontalAlignment="Left" Margin="197,33,0,0" VerticalAlignment="Top" Content="Логин" />
        <Label HorizontalAlignment="Left" Margin="197,74,0,0" VerticalAlignment="Top" Content="Пароль" />
        <Image Margin="10,10,0,0" Source="gibdd_logo.png" Stretch="Uniform" Height="152" VerticalAlignment="Top" Panel.ZIndex="-1" HorizontalAlignment="Left" Width="159"/>
    </Grid>
</Window>
